[
	{
		"Effect": "Allow",
		"Action": ["ingestion:readSource", "archive:read", "archive:search", "dashboard:read"],
		"Resource": ["ingestion-source/*", "archive/*", "dashboard/*"],
		"Condition": {
			"owner": true
		}
	},
	{
		"Effect": "Deny",
		"Action": [
			"ingestion:createSource",
			"ingestion:updateSource",
			"ingestion:deleteSource",
			"system:*"
		],
		"Resource": ["*"]
	}
]
